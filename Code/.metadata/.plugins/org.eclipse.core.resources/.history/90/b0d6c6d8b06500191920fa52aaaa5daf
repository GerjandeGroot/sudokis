#ifndef TYPEDEFS_H_
#define TYPEDEFS_H_

#define int8_t char

#define uint8_t unsigned char
#define uint16_t unsigned short
#define boolean unsigned char
#define uint32_t unsigned int

#define OUTPUT 1
#define INPUT 0
#define HIGH 1
#define LOW 0
#define max( a, b ) ( ((a) > (b)) ? (a) : (b) )


#endif /* TYPEDEFS_H_ */
